# 阿里云配置
# 在阿里云控制台 -> 访问控制 -> AccessKey 管理 中获取
ALIYUN_ACCESS_KEY_ID=你的AccessKeyId
ALIYUN_ACCESS_KEY_SECRET=你的AccessKeySecret

# 在阿里云 -> 智能语音交互 -> 项目管理 中获取 AppKey
ALIYUN_APPKEY=你的AppKey

# Supabase 配置
# 在 supabase.com -> 项目设置 -> API 中获取
SUPABASE_URL=https://你的项目ID.supabase.co
SUPABASE_SERVICE_KEY=你的service_role_key（不是anon_key）

# 服务端口（Render.com会自动设置PORT，本地开发用3000）
PORT=3000

